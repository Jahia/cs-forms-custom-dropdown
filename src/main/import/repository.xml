<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
    <modules jcr:primaryType="jnt:modules">
        <cs-forms-custom-dropdown j:dependencies="default form-factory-core graphql-dxm-provider"
                                j:modulePriority="0"
                                j:moduleType="module"
                                j:title="cs-forms-custom-dropdown"
                                jcr:mixinTypes="jmix:hasExternalProviderExtension"
                                jcr:primaryType="jnt:module">
                <sources jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                  jcr:primaryType="jnt:moduleVersionFolder"
                  j:staticMountPointProviderKey="module-cs-forms-custom-dropdown-1.0-SNAPSHOT">
                    <src jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                 jcr:primaryType="jnt:folder">
                        <main jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                     jcr:primaryType="jnt:folder">
                            <resources jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                             jcr:primaryType="jnt:folder">
                                <fcnt_selectPage jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                                      jcr:primaryType="jnt:nodeTypeFolder">
                                    <html jcr:mixinTypes="jmix:hasExternalProviderExtension"
                              jcr:primaryType="jnt:templateTypeFolder"/>
                                </fcnt_selectPage>
                                <fcnt_selectSite jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                                      jcr:primaryType="jnt:nodeTypeFolder">
                                    <html jcr:mixinTypes="jmix:hasExternalProviderExtension jmix:sourceControl"
                              jcr:primaryType="jnt:templateTypeFolder"/>
                                </fcnt_selectSite>
                            </resources>
                        </main>
                    </src>
                </sources>
                
                <portlets jcr:primaryType="jnt:portletFolder"/>
                <files jcr:primaryType="jnt:folder"/>
                <contents jcr:primaryType="jnt:contentFolder"/>
                <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
                    <files jcr:primaryType="jnt:folder"/>
                    <contents jcr:primaryType="jnt:contentFolder">
                        <form-factory-definitions jcr:primaryType="jnt:contentFolder">
                            <select-site jcr:mixinTypes="fcmix:extendedDefinition fcmix:layoutUUID"
                               jcr:primaryType="fcnt:selectSite"
                               layoutUUID="fakeUUID"
                               supportedLogicTypes="select valid"
                               supportedValidationTypes="required"
                               tab="select"
                               template="&lt;ff-select-site&gt;&lt;/ff-select-site&gt;"
                               wizard="&lt;ff-select-site view-type='designView'&gt;&lt;/ff-select-site&gt;">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Select Site"/>
                                <inputsize jcr:primaryType="fcnt:definitionOptions" jsonValue="input-md"/>
                                <helptext jcr:primaryType="fcnt:definitionOptionsTranslatable">
                                    <j:translation_en jcr:language="en"
                                          jcr:primaryType="jnt:translation"
                                          jsonValue="Help"/>
                                </helptext>
                                <placeholder jcr:primaryType="fcnt:definitionOptionsTranslatable">
                                    <j:translation_en jcr:language="en"
                                          jcr:primaryType="jnt:translation"
                                          jsonValue="Select a site"/>
                                </placeholder>
                            </select-site>
                            <select-page jcr:mixinTypes="fcmix:extendedDefinition fcmix:layoutUUID"
                               jcr:primaryType="fcnt:selectPage"
                               layoutUUID="fakeUUID"
                               supportedLogicTypes="select valid"
                               supportedValidationTypes="required"
                               tab="select"
                               template="&lt;ff-select-page&gt;&lt;/ff-select-page&gt;"
                               wizard="&lt;ff-select-page view-type='designView'&gt;&lt;/ff-select-page&gt;">
                                <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Select Page"/>
                                <inputsize jcr:primaryType="fcnt:definitionOptions" jsonValue="input-md"/>
                                <helptext jcr:primaryType="fcnt:definitionOptionsTranslatable">
                                    <j:translation_en jcr:language="en"
                                          jcr:primaryType="jnt:translation"
                                          jsonValue="Help"/>
                                </helptext>
                                <placeholder jcr:primaryType="fcnt:definitionOptionsTranslatable">
                                    <j:translation_en jcr:language="en"
                                          jcr:primaryType="jnt:translation"
                                          jsonValue="Select a page"/>
                                </placeholder>
                            </select-page>
                        </form-factory-definitions>
                    </contents>
                </templates>
                
            </cs-forms-custom-dropdown>
    </modules>
</content>